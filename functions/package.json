{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "emulator": "npm run build && firebase emulators:start",
    "deploy": "npm run swagger && npm run build && firebase deploy --only functions",
    "logs": "firebase functions:log",
    "swagger": "swaggerGen -c ./swaggerConfig.json",
    "testCircularDependencies": "madge src/index.ts",
    "test": "jest /functions"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@firebase/app": "^0.5.5",
    "@hapi/joi": "^16.1.8",
    "@types/express": "^4.17.3",
    "@types/joi": "^14.3.4",
    "cors": "latest",
    "express": "latest",
    "firebase": "^6.6.2",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "geofire": "^5.0.1",
    "joi-typescript-validator": "0.0.9",
    "lodash": "latest",
    "madge": "^3.7.0",
    "multer": "^1.4.2",
    "typescript-rest": "3.0.1",
    "typescript-rest-swagger": "^1.1.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/yup": "^0.26.32",
    "firebase-functions-test": "^0.1.6",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "private": true
}
